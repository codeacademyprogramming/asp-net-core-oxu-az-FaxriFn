
@{
    ViewData["Title"] = "Create";
    Layout = "~/Areas/NewsAdmin/Views/Shared/_Layout.cshtml";
}

@model News


<form method="post" autocomplete="off" enctype="multipart/form-data">
    <div class="form-group  my-5">

        <label asp-for="Photo"></label>
        <input asp-for="Photo" class="form-control" />
        <span asp-validation-for="Photo"></span>
    </div>
    <div class="form-group">

        <label asp-for="Title"></label>
        <input asp-for="Title" class="form-control" />
        <span asp-validation-for="Title"></span>
    </div>
    <div class="form-group">

        <label asp-for="CreateAt"></label>
        <input asp-for="CreateAt" class="form-control" />
        <span asp-validation-for="CreateAt"></span>
    </div>
    <div class="form-group">

        <label asp-for="Content"></label>
        <input asp-for="Content" class="form-control" />
        <span asp-validation-for="Content"></span>
    </div>
    <button type="submit" class="btn btn-outline-primary">Yarat</button>
</form>

